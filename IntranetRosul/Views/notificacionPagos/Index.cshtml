@model IEnumerable<IntranetRosul.Models.notificacionPagoes>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_BackOfficeCC.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
