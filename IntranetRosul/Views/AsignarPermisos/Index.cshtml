@model IEnumerable<IntranetRosul.Models.AsignarPermiso>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Backend.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ARoles.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.permisos.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Sistemas.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UsuarioId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ARoles.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.permisos.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sistemas.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UsuarioId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
