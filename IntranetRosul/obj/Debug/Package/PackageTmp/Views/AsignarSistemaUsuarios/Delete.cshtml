@model IntranetRosul.Models.AsignarSistemaUsuario

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Backend.cshtml";


}
    <!-- **********************************************************************************************************************************************************
        MAIN CONTENT
        *********************************************************************************************************************************************************** -->
    <!--main content start-->
    <section id="main-content">
        <section class="wrapper site-min-height">
            <h3><i class="fa fa-angle-right"></i>MANTENIMIENTO > SISTEMAS</h3>

            <h3>¿Esta seguro de eliminiar la asignacion?</h3>
            <div>

                <dl class="dl-horizontal">
                    <dt>
                        @Html.DisplayNameFor(model => model.Sistemas.Name)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Sistemas.Name)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.UserName)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.UserName)
                    </dd>

                </dl>

                @using (Html.BeginForm())
            {
                    @Html.AntiForgeryToken()

                    <div class="form-actions no-color">
                        <input type="submit" value="Delete" class="btn btn-primary" /> 
                        @Html.ActionLink("Regresar", "Index", new { @class = "btn btn-default" })
                    </div>
                }
            </div>

        </section>
    </section>
